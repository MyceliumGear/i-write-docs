#!/bin/sh
mv ./config/ansible/inventories/group_vars/stage ./config/ansible/inventories/group_vars/stage.old

gpg2 --decrypt --multifile \
     ./config/ansible/inventories/group_vars/stage.gpg

cksum ./config/ansible/inventories/group_vars/stage.old
cksum ./config/ansible/inventories/group_vars/stage
